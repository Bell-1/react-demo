{
	"presets": ["babel-preset-react-app"],
	"plugins": [
		["import", {
			"libraryName": "antd",
			"libraryDirectory": "es",
			"style": "css" // `style: true` 会加载 less 文件
		}],
		["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }]
	]
}